@model TestTask.Models.Room
@{
    ViewBag.Title = "Удаление адреса";
}

<div>
    <dl class="dl-horizontal">
        <dt>Страна</dt>
        <dd>
            @Html.DisplayFor(model => model.Country)
        </dd>
        <dt>Город</dt>
        <dd>
            @Html.DisplayFor(model => model.City)
        </dd>
        <dt>Улица</dt>
        <dd>
            @Html.DisplayFor(model => model.Street)
        </dd>
        <dt>Здание</dt>
        <dd>
            @Html.DisplayFor(model => model.Build)
        </dd>
        <dt>Офис\Квартира</dt>
        <dd>
            @Html.DisplayFor(model => model.Office)
        </dd>
    </dl>

    <div>
        <form asp-controller="Rooms" asp-action="Delete" method="post">
            <div class="form-group">
                <input type="submit" class="btn btn-default" value="Удалить" />
            </div>
        </form>
    </div>
</div>