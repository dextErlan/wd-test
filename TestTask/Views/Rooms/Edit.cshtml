@model TestTask.Models.Room
@{
    ViewBag.Title = "Редактирование адреса";
}
<form asp-action="edit" asp-controller="Rooms" asp-route-id="@Model.Id">
    <div class="form-group">
        <label asp-for="Country" class="control-label">Страна</label>
        <input type="text" asp-for="Country" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="City" class="control-label">Город</label>
        <input type="text" asp-for="City" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Street" class="control-label">Улица</label>
        <input type="text" asp-for="Street" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Build" class="control-label">Здание</label>
        <input type="text" asp-for="Build" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Office" class="control-label">Офис\Квартира</label>
        <input type="text" asp-for="Office" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Сохранить" class="btn btn-default" />
    </div>
</form>